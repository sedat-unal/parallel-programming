using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace bubblesort_parallel
{
    class Program
    {
        static void Main(string[] args)
        {
            double func = F(2, 4, 3);
            Console.WriteLine(func);
            Console.Read();
        }

        public static double F(double x1, double x2, double x3)
        {
            double t = 0;
            for (int i = 0; i < x2; i++)
            {
                t += x1 + i;
            }
            return t / (x3 * x3);
        }
    }
}
